<environment include="Development">
    <link rel="stylesheet" href="~/build/dist/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/build/dist/lib/font-awesome/css/font-awesome.css" />
    <link rel="stylesheet" href="~/build/dist/css/site.css" asp-append-version="true" />
</environment>
<environment exclude="Development">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous"
          asp-fallback-href="~/build/dist/lib/bootstrap/dist/css/bootstrap.min.css"
          asp-fallback-test-class="sr-only"
          asp-fallback-test-property="position"
          asp-fallback-test-value="absolute" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.0.13/css/all.css" integrity="sha384-DNOHZ68U8hZfKXOrtjWvjxusGo9WQnrNx2sqG0tfsghAvtVlRW3tvkXWZh58N9jp" crossorigin="anonymous"
          asp-fallback-href="~/build/dist/lib/font-awesome/css/font-awesome.min.css"
          asp-fallback-test-class="fa-stack-1x"
          asp-fallback-test-property="position"
          asp-fallback-test-value="absolute" />
    <link rel="stylesheet" href="~/build/dist/css/site.css" asp-append-version="true" />
</environment>
