{
  "compilerOptions": {
    "alwaysStrict": true,
    "declaration": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "es5",
    "lib": [
      "dom",
      "es2015"
    ],
    "sourceRoot": "ClientApp/code/ts",
    "outDir": "wwwroot/build/out/ts"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "compileOnSave": true
}
