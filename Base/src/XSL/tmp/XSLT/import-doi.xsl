	<xsl:template match="@*|node()">
		<xsl:param name="doi" select="''"/>
		<xsl:copy>
			<xsl:apply-templates select="@*"/>
			<xsl:apply-templates>
				<xsl:with-param name="doi">
					<xsl:value-of select="$doi"/>
				</xsl:with-param>
			</xsl:apply-templates>
		</xsl:copy>
	</xsl:template>

	<xsl:template match="node()[@object_id='95'][normalize-space(node()[@id='30']/value)='']">
		<xsl:param name="doi" select="''"/>
		<xsl:variable name="authors" select="normalize-space(.//node()[@id='495'])"/>
		<xsl:variable name="year" select="normalize-space(.//node()[@id='496'])"/>
		<xsl:variable name="content" select="normalize-space(.)"/>

		<xsl:variable name="vdoi">
			<xsl:for-each select="$list//item[contains($authors, substring(string(./authors),1,80))][contains($content, substring(string(./source),1,5))][contains(string(.//year), $year)]">
				<xsl:value-of select="doi"/>
				<xsl:if test="position()!=last()">
					<xsl:text> ### </xsl:text>
				</xsl:if>
			</xsl:for-each>
		</xsl:variable>

		<xsl:element name="{name()}">
			<xsl:apply-templates select="@*"/>
			<xsl:apply-templates>
				<xsl:with-param name="doi">
					<xsl:value-of select="$vdoi"/>
				</xsl:with-param>
			</xsl:apply-templates>
		</xsl:element>
	</xsl:template>

	<xsl:template match="node()[@id='30'][normalize-space(.)='']/value">
		<xsl:param name="doi" select="''"/>
		<xsl:element name="{name()}">
			<xsl:apply-templates select="@*"/>
			<xsl:value-of select="$doi"/>
		</xsl:element>
	</xsl:template>
</xsl:stylesheet>