  </entityFramework>
  <connectionStrings>
    <add name="GeoDbContext" connectionString="Data Source=(localdb)\ProjectsV12;Initial Catalog=GeoDatabase;Integrated Security=True" providerName="System.Data.SqlClient"/>
  </connectionStrings>
